{
    "name": "itpalert/web2sms-notification-channel",
    "description": "Web2sms Notification Channel for laravel.",
    "keywords": ["laravel", "notifications", "web2sms"],
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Gere Attila",
            "email": "gereattila98@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "itpalert/web2sms": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.2",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "ITPalert\\Web2smsChannel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ITPalert\\Web2smsChannel\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ITPalert\\Web2smsChannel\\Web2smsChannelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}